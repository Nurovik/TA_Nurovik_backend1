<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewsController</servlet-name>
        <servlet-class>Controllers.NewsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CmsNewsController</servlet-name>
        <servlet-class>Controllers.Cms.CmsNewsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CmsPemainController</servlet-name>
        <servlet-class>Controllers.Cms.CmsPemainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KlasmenController</servlet-name>
        <servlet-class>Controllers.Cms.KlasmenController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controllers.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>Controllers.Dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LigaController</servlet-name>
        <servlet-class>Controllers.LigaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PemainController</servlet-name>
        <servlet-class>Controllers.PemainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JadwalController</servlet-name>
        <servlet-class>Controllers.JadwalController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerKlasmen</servlet-name>
        <servlet-class>Controllers.ControllerKlasmen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Controllers.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewsController</servlet-name>
        <url-pattern>/NewsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CmsNewsController</servlet-name>
        <url-pattern>/CmsNewsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CmsPemainController</servlet-name>
        <url-pattern>/CmsPemainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KlasmenController</servlet-name>
        <url-pattern>/KlasmenController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/Dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LigaController</servlet-name>
        <url-pattern>/LigaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PemainController</servlet-name>
        <url-pattern>/PemainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JadwalController</servlet-name>
        <url-pattern>/JadwalController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerKlasmen</servlet-name>
        <url-pattern>/ControllerKlasmen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>CmsNewsController</welcome-file>
    </welcome-file-list>
</web-app>
